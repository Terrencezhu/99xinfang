/*banner内部搜索*/
.sousuo-box{width:886px;height:330px;margin: auto;position: absolute;top:0px;right:0;bottom:0;left:0;background:url(../img/indexb-back.png) no-repeat;z-index:100;}
#indexb-title{width:100%;height:102px;padding:48px 0;text-align: center;}
#sousuo{width:685px;height:50px;margin:auto;position: relative;}
#sousuo-text{width:565px;height:50px;text-indent: 18px;font-size: 14px;color:#333;background:rgba(255,255,255,.2);border:1px solid #e14122;line-height: 50px;border-radius:4px;}
#sousuo-submit{width:120px;height:53px;font-size:18px;background:#e14122;border-top-right-radius: 4px;border-bottom-right-radius: 4px;line-height: 53px;position: absolute;top:-1px;right:0px;z-index: 9999;}
.sousuo-tuijian{width:685px;height: 20px;margin:20px auto 0;line-height: 20px;position: relative;}
.sousuo-tuijian a{float:left;font-size: 14px;color:#999;border-right:1px solid #999;padding-right:15px;margin-right:15px;}
.sousuo-tuijian a:last-child{border:none;}
#banner-tel{width:200px;position: absolute;top:0;right:-60px;font-size: 14px;color:#e14122;font-weight: bold;}
#banner-tel i{display:inline-block;width:22px;height:20px;background:url(../img/background.png) -38px 0 no-repeat;vertical-align: -28%;margin-right: 2px;}
/*平台优势merit*/
#merit-tag{width:1100px;height:40px;margin:auto;}
#merit-tag li{width:150px;height:100%;line-height:40px;margin-right: 40px;float:left;background: #f5f5f5;text-align: center;border-radius:5px;font-size: 16px;color: #666;cursor: pointer;}
.merit{width:1100px;height:340px;margin:40px auto;display: none;}
.merit-img,.merit-img img{width:590px;height:100%;float:left;}
.merit-right{width:475px;height:100%;float:right;color:#333;position: relative;}
.merit-title{font-size: 20px;padding:20px 0 30px;}
.merit-text{font-size:14px;line-height: 25px;}
.meritl-arrow,.meritr-arrow{width:34px;height:46px;position:absolute;bottom:45px;cursor: pointer;}
.meritl-arrow{left:0;background: url(../img/background.png) -280px 0 no-repeat;}
.meritr-arrow{right:0;background: url(../img/background.png) -232px 0 no-repeat;}
/*楼盘推荐fine*/
.fine-box{width:100%;height:800px;background: #f5f5f5;padding:1px 0 40px;}
.fine{width:1100px;margin:auto;height:350px;}
.fine a{display: block;}
.fine a:hover{color:#e14122;}
.fine li{width:353px;height:auto;float:left;}
.fine-margin{margin-right:20px;}
.fine-img{width:100%;height:230px;position: relative;overflow: hidden;}
.fine-img img{width:100%;height:100%;position: absolute;left:0;top:0;}
.fine-title{margin-top:15px;font-size: 16px;color:#333;}
.fine-title i{display: inline-block;width:12px;}
.fine-xinxi,.fine-time{font-size:14px;color:#666;margin-top: 15px;}
.fine-xinxi i{display: inline-block;width:8px;}
.fine-time{padding-bottom: 30px;}
.fine-img img:hover{animation:fine 1s;-moz-animation:fine 1s;-webkit-animation:fine 1s;-ms-animation:fine 1s;}
@keyframes fine{
	from{width:100%;height:100%;top:0;left:0;}
	to{width:103%;height:103%;top:-3%;left:-3%;}
}
@-moz-keyframes fine{
	from{width:100%;height:100%;top:0;left:0;}
	to{width:103%;height:103%;top:-3%;left:-3%;}
}
@keyframes fine{
	from{width:100%;height:100%;top:0;left:0;}
	to{width:103%;height:103%;top:-3%;left:-3%;}
}
@-webkit-keyframes fine{
	from{width:100%;height:100%;top:0;left:0;}
	to{width:103%;height:103%;top:-3%;left:-3%;}
}
@-ms-keyframes fine{
	from{width:100%;height:100%;top:0;left:0;}
	to{width:103%;height:103%;top:-3%;left:-3%;}
}
/*新闻动态 news*/
#news{width:1100px;height:auto;margin:auto;position: relative;}
#news-left{width:390px;height:auto;}
.news-img{display:block;width:100%;height:217px;margin-bottom: 40px;}
.news-img img{width:100%;height:100%;}
.news-title{display:block;width:100%;position: relative;margin-bottom: 20px;}
.news-titles{color:#333;font-size:16px;font-weight:bold;}
.news-titles:hover{color:#e14122;}
.news-time{position: absolute;top:3px;right:0;color:#666;font-size: 12px;}
.news-text{font-size: 12px;color:#666;text-indent: 20px;line-height: 20px;}
.news-text a{color:#e14122;}
#news-right{position: absolute;top:0;right:0;width:660px;height:auto;}
.news-list{width:100%;height:auto;padding-bottom:20px;border-bottom: 1px solid #e2e1e0;margin-bottom: 20px;}
.news-lists{padding-bottom: 0;border:0;}
/*关于99新房*/
#touch{width:1100px;margin:0 auto 40px;height:300px;}
.touch-list{width:540px;height:300px;border-radius: 5px;box-shadow: 2px 1px 3px rgba(0,0,0,.1);float:left;}
.touch-lists{margin-right:20px;}
.touch-img{width:200px;height:100%;float:left;margin-right: 40px;}
.touch-img img{width:100%;height:100%;}
.touch-right{width:300px;height:100%;float:left;}
.touch-bold{font-size:24px;color:#7293d7;padding:30px 0 10px;}
.touch-bolds{color:#e14122;}
.touch-tel{font-size:26px;color:#333;margin-bottom: 5px;}
.touch-conter{height:80px;}
.touch-weixin{width:80px;height:80px;float: left;}
.touch-weixin img{width:100%;height:100%;}
.touch-conter ul{width:200px;float:left;font-size:14px;margin-left: 20px;color:#666;}
.touch-conter ul li{margin-bottom: 5px;}
.touch-conter-text{color:#333;font-size: 14px;margin-bottom: 20px;}
.touch-conter-button{width:130px;height:30px;line-height:30px;text-align:center;border:1px solid #e14122;border-radius: 20px;color:#e14122;font-size:14px;cursor: pointer;}
.touch-time{font-size: 12px;color:#666;}
/*tol底部列表*/
.tol-box{height:281px;background: #f5f5f5;background: url(../img/tol.jpg) no-repeat;}
#tol{width:1100px;padding-top:60px;height:155px;margin:auto;}
#tol li{width:275px;height:100%;float:left;text-align: center;}
.tol-img{margin-bottom:26px;}
.tol-text{font-size: 18px;color:#333;}
/*申请合作*/
#hezuo-background{width:100%;height:100%;position: fixed;top:0;right:0;bottom:0;left:0;background: #000;z-index:500;opacity: 0.9;filter:alpha(opacity=90);display: none;}
#hezuo-fixed{width:910px;height:290px;background: #fff;margin: auto;position:fixed;top:0;right:0;bottom:0;left:0;z-index: 501;padding:30px 60px;display: none;}
#img-guanbi{width:32px;height:30px;position: absolute;top:5%;right:5%;cursor: pointer;z-index:1002;background: url(../img/guanbi.png) no-repeat;}
.hezuo-title{font-size:24px;color:#666;margin-bottom:5px;}
.hezuo-text{font-size:16px;color:#999;margin-bottom: 6px;}
#hezuo-left{width:340px;height:100%;border-right:1px solid #e2e1e0;float:left;}
#hezuo-right{width:508px;height:100%;float: right;}
.hezuo-input-box{width:100%;height:auto;margin-bottom:20px;}
.hezuo-input:nth-child(2){margin-left:47px;}
#hezuo-right input[type="text"]{width:226px;height:36px;line-height:36px;border-radius: 4px;color:#999;border:1px solid #e2e1e0;text-indent: 10px;font-size: 14px;}
#hezuo-right textarea{width:100%;height:114px;border:1px solid #e2e1e0;outline: none;resize: none;color:#333;text-indent: 10px;line-height: 20px;font-size: 14px;}
#hezuo-right input[type="submit"]{width:236px;height:38px;line-height: 38px;background: #e14122;color:#fff;font-size: 16px;border-radius: 4px;margin-top: 20px;}









